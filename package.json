{
  "name": "wxml-class-name-to-wxss",
  "displayName": "className Completion in wxss and wxml",
  "description": "微信小程序wxml与wxss双向类名提示；wxss中提示全局样式page标签定义的变量；为wxml文件增加符号表",
  "publisher": "wjf",
  "repository": {
    "url": "https://github.com/wjf2016/wxmlClassNameToWxss.git"
  },
  "icon": "icon.jpg",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.42.0"
  },
  "license": "MIT",
  "author": {
    "name": "阿福"
  },
  "categories": [
    "Programming Languages",
    "Other"
  ],
  "keywords": [
    "IntelliSense",
    "completion",
    "autocomplete",
    "class",
    "wxss",
    "wxml",
    "classtowxss",
    "class to wxss",
    "classnametowxss",
    "classname to wxss"
  ],
  "activationEvents": [
    "workspaceContains:**/*.wxss",
    "workspaceContains:**/*.wxml"
  ],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "dependencies": {
    "@wxml/generator": "^0.1.0",
    "@wxml/parser": "^0.4.0",
    "css-tree": "^2.3.1"
  },
  "devDependencies": {
    "@types/node": "^10.12.21",
    "@types/vscode": "^1.42.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "typescript": "^4.2.3",
    "vscode": "^1.1.37"
  }
}
